<div class="ui tariffs modal">
  <i class="close icon"></i>
  <div class="header">
    <i class="file invoice dollar icon"></i>
    &nbsp;Relação de tarifas cobradas
  </div>
  <div class="scrolling content">
    <div class="description">
      <div class="ui header">
        <div class="sub header">
          Título
        </div>
      </div>
      <table class="ui celled tariffs compact table" id="tariffsTable">
        <thead>
          <tr>
            <th class="center aligned">Data</th>
            <th class="center aligned">Tarifa cobrada</th>
            <th class="center aligned">Valor</th>
          </tr>
        </thead>
        <tbody>
        </tbody>
        <tfoot class="full-width">
          <tr>
            <th colspan="2" class="bold right aligned">
              Valor total das tarifas cobradas:
            </th>
            <th class="bold right aligned">
              <span class="symbol">R$</span>
              <span class="bold" id="total">0,00</span>
            </th>
          </tr>
          <tr>
            <th colspan="3">
              <button class="ui right floated small primary labeled icon button"
                      onclick="tableToExcel('tariffsTable', 'Tarifas cobradas')">
                <i class="file excel icon"></i>
                Exportar para Excel
              </button>
            </th>
          </tr>
        </tfoot>
      </table>
    </div>
  </div>
  <div class="actions">
    <div class="ui positive right labeled icon button">
      Fechar
      <i class="checkmark icon"></i>
    </div>
  </div>
</div>